[
  {
    "name": "Webhook → Google Sheets #BTC",
    "tags": [
      "webhook",
      "sheets",
      "btc",
      "append"
    ],
    "summary": "Capture data via webhook and append a row into Google Sheets.",
    "json": {
      "name": "Webhook → Google Sheets #BTC",
      "nodes": [
        {
          "id": "wh32",
          "name": "Webhook",
          "type": "n8n-nodes-base.webhook",
          "typeVersion": 1,
          "position": [
            200,
            200
          ],
          "parameters": {
            "path": "capture_32",
            "responseMode": "onReceived"
          }
        },
        {
          "id": "gs32",
          "name": "Append to Sheet",
          "type": "n8n-nodes-base.googleSheets",
          "typeVersion": 1,
          "position": [
            520,
            200
          ],
          "parameters": {
            "operation": "append",
            "sheetId": "={{$env.SHEET_ID}}",
            "range": "={{$env.SHEET_RANGE}}"
          }
        }
      ],
      "connections": {
        "wh32": {
          "main": [
            [
              {
                "node": "gs32",
                "type": "main",
                "index": 0
              }
            ]
          ]
        }
      },
      "settings": {},
      "tags": [
        "template"
      ]
    }
  }
]